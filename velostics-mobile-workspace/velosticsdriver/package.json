{
  "name": "velostics-driver",
  "scripts": {
    "android": "react-native run-android",
    "android-test": "ENVFILE=.env.testing yarn android",
    "ios": "react-native run-ios --simulator \"iPhone 8\"",
    "ios-test": "ENVFILE=.env.testing yarn ios",
    "web": "expo start --web",
    "start": "react-native start",
    "build-dev-android": "cd android &&  ENVFILE=.env ./gradlew assembleRelease",
    "build-testing-android": "cd android &&  ENVFILE=.env.testing ./gradlew assembleRelease",
    "build-staging-android": "cd android &&  ENVFILE=.env.staging ./gradlew assembleRelease",
    "build-production-android": "cd android &&  ENVFILE=.env.production ./gradlew assembleRelease",
    "release-staging-ios": "appcenter codepush release-react -a Velostics/velostics-driver-ios --plist-file \"ios/velosticsdriver/Info.plist\" -d Staging",
    "release-production-ios": "appcenter codepush release-react -a Velostics/velostics-driver-ios --plist-file \"ios/velosticsdriver/Info.plist\" -d Production",
    "release-testing-ios": "appcenter codepush release-react -a Velostics/velostics-driver-ios --plist-file \"ios/velosticsdriver/Info.plist\" -d Testing",
    "release-staging-android": "appcenter codepush release-react -a Velostics/velostics-driver-android -d Staging",
    "release-production-android": "appcenter codepush release-react -a Velostics/velostics-driver-android -d Production",
    "release-testing-android": "appcenter codepush release-react -a Velostics/velostics-driver-android -d Testing",
    "release-dev-android": "appcenter codepush release-react -a Velostics/velostics-driver-android -d Dev",
    "release-dev-ios": "appcenter codepush release-react -a Velostics/velostics-driver-ios --plist-file \"ios/velosticsdriver/Info.plist\" -d Dev",
    "display-keys-ios": "appcenter codepush deployment list -a Velostics/velostics-driver-ios --displayKeys",
    "display-keys-android": "appcenter codepush deployment list -a Velostics/velostics-driver-android --displayKeys",
    "release-test": "yarn release-testing-ios && yarn release-testing-android",
    "release-staging": "yarn release-staging-android && yarn release-staging-ios",
    "release-production": "yarn release-production-android && yarn release-production-ios",
    "test": "jest",
    "menu": "adb shell input keyevent KEYCODE_MENU",
    "postinstall": "jetify && mkdir -p ./node_modules/@velostics/shared && cp -a ../shared/. ./node_modules/@velostics/shared/"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.4.1",
